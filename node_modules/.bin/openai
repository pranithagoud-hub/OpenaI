#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/.pnpm/openai@5.20.2/node_modules/openai/bin/cli/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/.pnpm/openai@5.20.2/node_modules/openai/bin/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/.pnpm/openai@5.20.2/node_modules/openai/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/.pnpm/openai@5.20.2/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/openai/bin/cli/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/openai/bin/node_modules:/home/nxtwave/reactjs/sessions/Introduction-to-React-JS/open-ai/node_modules/openai/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else 
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
